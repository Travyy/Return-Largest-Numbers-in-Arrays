function largestOfFour(arr) {
  var largestNumArr = [];
  
  for (i = 0; i < arr.length; ++i)  {
     var counter = 0;
     for (j = 0; j < arr[i].length; ++j)  {
       if (arr[i][j] > counter)  {
         counter = arr[i][j];
       }
     }
    largestNumArr[largestNumArr.length] = counter;
  }
  
  return largestNumArr;
}

largestOfFour([[13, 27, 18, 26], [4, 5, 1, 3], [32, 35, 37, 39], [1000, 1001, 857, 1]])
